<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>
      {% block title %}{% endblock %}
    </title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    {% block stylesheets %}{% endblock %}
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
      <div class="container">
        <a class="navbar-brand" href="{{path('home')}}">Home</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link {% if current_menu is defined and current_menu == 'agencies' %}  active {% endif %}" href="{{path('agency')}}">Agency
              </a>
            </li>

            <li class="nav-item">
              <a class="nav-link {% if current_menu is defined and current_menu == 'blogs' %}  active {% endif %}" href="{{path('blog')}}">Blog</a>
            </li>
            <li class="nav-item">
              <a class="nav-link {% if current_menu is defined and current_menu == 'developments' %}  active {% endif %}" href="{{path('devhome')}}">Development</a>
            </li>
            <li class="nav-item">
              <a class="nav-link {% if current_menu is defined and current_menu == 'admins' %}  active {% endif %}" href="{{path('admin.property.index')}}">Admin</a>
            </li>
          </ul>
          {% if app.user %}
          <ul class="navbar-nav">
<li class="nav-item">
<a href="{{ path('logout') }}" class="nav-link">Se deconnecter</a>
</li>
</li>
          </ul>
          {% endif %}
        </div>
      </div>
    </nav>
 <header class="bg-primary py-5">
  </header>

    {% block body %}{% endblock %}



    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    {% block javascripts %}{% endblock %}
  </body>
</html>
